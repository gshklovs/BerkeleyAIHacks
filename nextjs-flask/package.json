{
  "name": "next-flask",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "flask-dev": "FLASK_DEBUG=1 source .venv/bin/activate && pip3 install -r requirements.txt && python3 -m flask --app api/index run -p 5328",
    "flask-dev-win": "cmd /C \".venv\\Scripts\\activate.bat && pip install -r requirements.txt && python -m flask --app api/index run -p 5328\"",
    "next-dev": "next dev",
    "dev": "concurrently \"pnpm run next-dev\" \"pnpm run flask-dev\"",
    "dev-win": "concurrently \"pnpm run next-dev\" \"pnpm run flask-dev-win\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@types/node": "20.1.4",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "axios": "^1.7.2",
    "clsx": "^2.1.1",
    "concurrently": "^8.0.1",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "framer-motion": "^11.2.11",
    "next": "13.4.3",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "socket.io-client": "^4.7.5",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.4",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": "18.x"
  }
